local Mm = Tab3:CreateSection("Movement",false)
local walkspeed
local delrunning = false
Tab3:CreateSlider({
	Name = "Walk Speed (0 for default running)",
	   	Interact = 'Interact',
	SectionParent = Mm,
	Range = {0, 500},
	Increment = 10,
	Suffix = "",
	CurrentValue = 0,
	Flag = "Walk Speed",
	Callback = function(Value)
		walkspeed = Value
        if Value ~= 0 then
            delrunning = true
            game.Players.LocalPlayer.Character.Humanoid.WalkSpeed = Value
        end
	end,
})
spawn(function() -- sorry for dumb code but im honestly clueless because of the way the character model works
    while wait(5) do
        if delrunning then
            game.Players.LocalPlayer.Character.Humanoid.WalkSpeed = walkspeed
            pcall(function() game.Workspace:FindFirstChild("Running",1):Destroy() end)
        end
    end
end)

Tab3:CreateToggle({
	Name = "Inf Jump",
		   	Interact = 'Interact',
	SectionParent = Mm,
	CurrentValue = false,
	Flag = "Toggle50", 
	Callback = function(Value)
    antiAfk = Value
            while antiAfk do task.wait(0.1)
local Player = game:GetService'Players'.LocalPlayer;
local UIS = game:GetService'UserInputService';
 
_G.JumpHeight = 50;
 
function Action(Object, Function) if Object ~= nil then Function(Object); end end
 
UIS.InputBegan:connect(function(UserInput)
    if UserInput.UserInputType == Enum.UserInputType.Keyboard and UserInput.KeyCode == Enum.KeyCode.Space then
        Action(Player.Character.Humanoid, function(self)
            if self:GetState() == Enum.HumanoidStateType.Jumping or self:GetState() == Enum.HumanoidStateType.Freefall then
                Action(self.Parent.HumanoidRootPart, function(self)
                    self.Velocity = Vector3.new(0, _G.JumpHeight, 0);
                end)
            end
        end)
    end
end)
		end
	end,
})

