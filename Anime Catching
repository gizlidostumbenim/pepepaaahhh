	Name = "Selected Hatch",
		SectionParent = S2,
	CurrentValue = false,
	CurrentValue = false,
	Flag = "Selected Hatch",
	Callback = function(value)
	getgenv().autoHatch = value
	if value then
            doHatch();
        end
	end,
})

function doHatch()
spawn(function()
        while autoHatch and wait() do
    if getgenv().eggType == "Egg 1" then
local args = {[1] = 7000001,[2] = 1}game:GetService("ReplicatedStorage").Tool.DrawUp.Msg.DrawHero:InvokeServer(unpack(args))
   
    elseif getgenv().eggType == "Egg 2" then
local args = {[1] = 7000002,[2] = 1}game:GetService("ReplicatedStorage").Tool.DrawUp.Msg.DrawHero:InvokeServer(unpack(args))
   
   elseif getgenv().eggType == "Egg 3" then
local args = {[1] = 7000003,[2] = 1}game:GetService("ReplicatedStorage").Tool.DrawUp.Msg.DrawHero:InvokeServer(unpack(args))
    
    elseif getgenv().eggType == "Egg 4" then
local args = {[1] = 7000004,[2] = 1}game:GetService("ReplicatedStorage").Tool.DrawUp.Msg.DrawHero:InvokeServer(unpack(args))
   
    elseif getgenv().eggType == "Egg 5" then
local args = {[1] = 7000005,[2] = 1}game:GetService("ReplicatedStorage").Tool.DrawUp.Msg.DrawHero:InvokeServer(unpack(args))
   
    elseif getgenv().eggType == "Egg 6" then
local args = {[1] = 7000006,[2] = 1}game:GetService("ReplicatedStorage").Tool.DrawUp.Msg.DrawHero:InvokeServer(unpack(args))
    
    elseif getgenv().eggType == "Egg 7" then
local args = {[1] = 7000007,[2] = 1}game:GetService("ReplicatedStorage").Tool.DrawUp.Msg.DrawHero:InvokeServer(unpack(args))
   
   elseif getgenv().eggType == "Egg 8" then
local args = {[1] = 7000008,[2] = 1}game:GetService("ReplicatedStorage").Tool.DrawUp.Msg.DrawHero:InvokeServer(unpack(args))
    
    elseif getgenv().eggType == "Egg 9" then
local args = {[1] = 7000009,[2] = 1}game:GetService("ReplicatedStorage").Tool.DrawUp.Msg.DrawHero:InvokeServer(unpack(args))
    
    elseif getgenv().eggType == "Egg 10" then
local args = {[1] = 70000010,[2] = 1}game:GetService("ReplicatedStorage").Tool.DrawUp.Msg.DrawHero:InvokeServer(unpack(args))

    elseif getgenv().eggType == "Egg 11" then
local args = {[1] = 70000011,[2] = 1}game:GetService("ReplicatedStorage").Tool.DrawUp.Msg.DrawHero:InvokeServer(unpack(args))

    elseif getgenv().eggType == "Egg 12" then
local args = {[1] = 70000012,[2] = 1}game:GetService("ReplicatedStorage").Tool.DrawUp.Msg.DrawHero:InvokeServer(unpack(args))

    elseif getgenv().eggType == "Egg 13" then
