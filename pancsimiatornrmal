local openegg
Tab5:CreateToggle({
	Name = "Open Egg World",
	CurrentValue = false,
	Callback = function(Value)
		openegg = Value
        if Value then
            spawn(function()
                openegg_func()
            end)
        end
	end,
})




function openegg_func()
    spawn(function()
        while wait(0.1) and openegg do
local args = 
   eggtoopen
game:GetService("ReplicatedStorage").Events.PlayerPressedKeyOnEgg:FireServer(unpack(args))
        end
    end)
end


