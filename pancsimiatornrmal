function caganss()
    local button = game:service'VirtualInputManager'
    local X, Y = 0, 0

    button:SendKeyEvent(true, "E", false, game)
    wait(0.1)
end

 function tp(PCFrame)
    local plr = game.Players.LocalPlayer;
           if plr.Character then
            plr.Character.HumanoidRootPart.CFrame = PCFrame
        end
    end 
