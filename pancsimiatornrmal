Tab:CreateToggle({
    Name = "Auto Farm Dungeon Area V2",
    CurrentValue = false,
    Callback = function(Value)
        b = Value
        while b do
            task.wait()
            repeat
                task.wait()
            until game.Players.LocalPlayer.Character ~= nil
            local mob = getClosestMob()
            if mob then
                local tweenInfo = TweenInfo.new(0.1, Enum.EasingStyle.Linear, Enum.EasingDirection.Out)
                local tween = game:GetService("TweenService"):Create(game.Players.LocalPlayer.Character.HumanoidRootPart, tweenInfo, {CFrame = mob.CFrame * CFrame.new(0,0,4)})
                tween:Play()
            end
        end
    end,
})


