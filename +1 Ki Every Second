Tab1:CreateToggle({
	Name = "Selected Hatch",
	CurrentValue = false,
	Flag = "Selected Hatch",
	Callback = function(value)
	getgenv().autoHatch = value
	if value then
            doHatch();
        end
	end,
})

function doHatch()
spawn(function()
        while autoHatch and wait() do
    if getgenv().eggType == "Egg 1" then
local args = {[1] = "draw001",[2] = 1}game:GetService("ReplicatedStorage").Events.Player.Draw.DrawRequest:FireServer(unpack(args))
