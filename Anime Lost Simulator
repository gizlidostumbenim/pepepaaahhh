	Name = "Selected Hatch",
		SectionParent = S2,
	CurrentValue = false,
	Flag = "Selected Hatch",
	Callback = function(value)
	getgenv().autoHatch = value
	if value then
            doHatch();
        end
	end,
})

function doHatch()
spawn(function()
        while autoHatch and wait() do
    if getgenv().eggType == "Egg 1" then
local args = {[1] = 9999,[2] = "Open_1"}game:GetService("ReplicatedStorage").Modules.LBConnection.Remotes.Egg_Spin.Sent:FireServer(unpack(args))
    elseif getgenv().eggType == "Egg 2" then
local args = {[1] = 9999,[2] = "Open_2"}game:GetService("ReplicatedStorage").Modules.LBConnection.Remotes.Egg_Spin.Sent:FireServer(unpack(args))
    elseif getgenv().eggType == "Egg 3" then
local args = {[1] = 9999,[2] = "Open_3"}game:GetService("ReplicatedStorage").Modules.LBConnection.Remotes.Egg_Spin.Sent:FireServer(unpack(args))
    elseif getgenv().eggType == "Egg 4" then
local args = {[1] = 9999,[2] = "Open_4"}game:GetService("ReplicatedStorage").Modules.LBConnection.Remotes.Egg_Spin.Sent:FireServer(unpack(args))
