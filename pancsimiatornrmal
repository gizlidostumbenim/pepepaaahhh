local function getClosestMob()
    local dist, thing = math.huge
    for i, v in next, game:GetService("Workspace").HiddenChests:GetDescendants() do
        if v.Name == "Sparkle" then
            local mag = (game.Players.LocalPlayer.Character.HumanoidRootPart.Position - v.Position).magnitude
            if mag < dist then
                dist = mag
                thing = v
            end
        end
    end
    if thing then
        return thing
    else
        return nil
    end
end


Tab:CreateToggle({
    Name = "Auto Farm Chest",
    CurrentValue = false,
    Callback = function(Value)
        ba = Value
        while ba do
        caganss()
            task.wait()
            repeat
                task.wait()
            until game.Players.LocalPlayer.Character ~= nil
            local moba = getClosestMob()
            if moba then
                local tweenInfo = TweenInfo.new(0.5, Enum.EasingStyle.Linear, Enum.EasingDirection.Out)
                local tween = game:GetService("TweenService"):Create(game.Players.LocalPlayer.Character.HumanoidRootPart, tweenInfo, {CFrame = moba.CFrame * CFrame.new(0,0,2)})
                tween:Play()
            end
        end
    end,
})

local function getClosestMob()
    local dist, thing = math.huge
    for i, v in next, game:GetService("Workspace").BreakableParts.Dungeon:GetDescendants() do
        if v.Name == "Head" then
            local mag = (game.Players.LocalPlayer.Character.HumanoidRootPart.Position - v.Position).magnitude
            if mag < dist then
                dist = mag
                thing = v
            end
        end
    end
    if thing then
        return thing
    else
        return nil
    end
end

