local TriggerBotDistance = math.huge	
	
	Universal:CreateSlider({
		Name = "Distance",
		Range = {
			0,
			5000
		},
		Increment = 1,
		Suffix = "",
		CurrentValue = 25,
		Flag = "Walk Speeda",
		Callback = function(value)
        TriggerBotDistance = value * 5.4321
		end
	})	
	
