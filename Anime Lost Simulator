	Callback = function(Value)
local args = {[1] = "LowVFX",[2] = true}game:GetService("ReplicatedStorage").Modules.LBConnection.Remotes.changeSetting:FireServer(unpack(args))
local args = {[1] = "DisableOthersVFX",[2] = true}game:GetService("ReplicatedStorage").Modules.LBConnection.Remotes.changeSetting:FireServer(unpack(args))
local args = {[1] = "HidePets",[2] = true}game:GetService("ReplicatedStorage").Modules.LBConnection.Remotes.changeSetting:FireServer(unpack(args))
local args = {[1] = "FastMode",[2] = true}game:GetService("ReplicatedStorage").Modules.LBConnection.Remotes.changeSetting:FireServer(unpack(args))
	end,
})

Tab5:CreateSection("Movement")

Tab5:CreateToggle({
	Name = "Inf Jump",
	CurrentValue = false,
	Flag = "Toggle50", 
	Callback = function(Value)
    antiAfk = Value
            while antiAfk do task.wait(0.1)
local Player = game:GetService'Players'.LocalPlayer;
local UIS = game:GetService'UserInputService';
 
_G.JumpHeight = 50;
 
function Action(Object, Function) if Object ~= nil then Function(Object); end end
 
UIS.InputBegan:connect(function(UserInput)
    if UserInput.UserInputType == Enum.UserInputType.Keyboard and UserInput.KeyCode == Enum.KeyCode.Space then
        Action(Player.Character.Humanoid, function(self)
            if self:GetState() == Enum.HumanoidStateType.Jumping or self:GetState() == Enum.HumanoidStateType.Freefall then
                Action(self.Parent.HumanoidRootPart, function(self)
                    self.Velocity = Vector3.new(0, _G.JumpHeight, 0);
                end)
            end
        end)
    end
end)
		end
	end,
})

local walkspeed
local delrunning = false
Tab5:CreateSlider({
	Name = "Walk Speed (0 for default running)",
	Range = {0, 500},
	Increment = 10,
	Suffix = "",
	CurrentValue = 0,
	Flag = "Walk Speed",
	Callback = function(Value)
		walkspeed = Value
        if Value ~= 0 then
            delrunning = true
            game.Players.LocalPlayer.Character.Humanoid.WalkSpeed = Value
        end
	end,
})

local VirtualUser=game:service'VirtualUser'
Tab5:CreateToggle({
	Name = "Anti Afk",
	CurrentValue = true,
	Flag = "Toggle50", 
	Callback = function(Value)
    antiAfk = Value
            while antiAfk do task.wait()
game:service'Players'.LocalPlayer.Idled:connect(function()
VirtualUser:CaptureController()
VirtualUser:ClickButton2(Vector2.new())
end)
		end
	end,
})

