Tab1:CreateToggle({
	Name = "Selected Upgrade All",
	CurrentValue = false,
	Flag = "Selected Farm",
	Callback = function(state)
    if state then
        if lp.PlayerGui.Feedback:FindFirstChild("Verification") then
            lp.PlayerGui.Feedback.Verification:Destroy()
        end
        getgenv().AUA = state
