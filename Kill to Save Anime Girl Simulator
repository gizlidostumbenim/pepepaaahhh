local client = game.Players.LocalPlayer
local insta = game.Players.LocalPlayer.Character
local Rayfield = loadstring(game:HttpGet('https://raw.githubusercontent.com/shlexware/Rayfield/main/source'))()

local Window = Rayfield:CreateWindow({
   Name = "Kill to Save Anime Girl Simulator",
   LoadingTitle = "Starting...",
   LoadingSubtitle = "by OXYGEN",
   ConfigurationSaving = {
      Enabled = true,
      FolderName = nil, -- Create a custom folder for your hub/game
      FileName = "Hub"
   },
   Discord = {
      Enabled = false,
      Invite = "ABCD", -- The Discord invite code, do not include discord.gg/
      RememberJoins = true -- Set this to false to make them join the discord every time they load it up
   },
   KeySystem = false, -- Set this to true to use our key system
   KeySettings = {
      Title = "Oxygen Hub",
      Subtitle = "Key System",
      Note = "Join the discord (discord.gg/)",
      FileName = "Key",
      SaveKey = true,
      GrabKeyFromSite = false, -- If this is true, set Key below to the RAW site you would like Rayfield to get the key from
      Key = "FUCKME"
   }
})
local Tab = Window:CreateTab("Auto Farm", 4483362458)

Tab:CreateSection("Farming")
function autoattack()
    local button = game:service'VirtualInputManager'
    local X, Y = 0, 0
    
    button:SendMouseButtonEvent(X, Y, 0, true, game, 1)
    button:SendMouseButtonEvent(X, Y, 0, false, game, 1)
    wait(0.3)
end
Tab:CreateToggle({
	Name = "Auto Attack",
	CurrentValue = false,
	Flag = "Toggle2", 
	Callback = function(Value)
		basd = Value
            while basd do task.wait()
                autoattack()
		end
	end,
})

local Dropdown = Tab:CreateDropdown({
   Name = "Select Stats",
   Options = {"Damage","Health","Speed","Luck"},
   CurrentOption = "Select",
   Flag = "Select Box", 
   Callback = function(value)
    getgenv().eggType1 = value
    end,
}) 



Tab:CreateToggle({
	Name = "Selected Add Stats",
	CurrentValue = false,
	Flag = "Selected Hatch",
	Callback = function(value)
	getgenv().autoHatch1 = value
	if value then
            doHatch1();
        end
	end,
})

function doHatch1()
spawn(function()
        while autoHatch1 and wait() do
    if getgenv().eggType1 == "Damage" then
local args = {[1] = "STAT_INCREASE",[2] = "Damage"}game:GetService("ReplicatedStorage").Actions.Action:FireServer(unpack(args))
    elseif getgenv().eggType1 == "Health" then
local args = {[1] = "STAT_INCREASE",[2] = "Health"}game:GetService("ReplicatedStorage").Actions.Action:FireServer(unpack(args))
    elseif getgenv().eggType1 == "Speed" then
local args = {[1] = "STAT_INCREASE",[2] = "Speed"}game:GetService("ReplicatedStorage").Actions.Action:FireServer(unpack(args))
    elseif getgenv().eggType1 == "Luck" then
local args = {[1] = "STAT_INCREASE",[2] = "Luck"}game:GetService("ReplicatedStorage").Actions.Action:FireServer(unpack(args))
        
            end
        end
    end)
end

function TPTo(PCFrame)
    local plr = game.Players.LocalPlayer;
           if plr.Character then
            plr.Character.HumanoidRootPart.CFrame = PCFrame
        end
    end



for i, v in pairs(game:GetService("Workspace").SpawnedEnemies:GetDescendants()) do
   if v.Name == "Cheat" then
       v:Destroy()
   end
end


local automob = Tab:CreateToggle({
	Name = "Auto Teleport Mob",
	CurrentValue = false,
	Flag = "Toggle2", 
	Callback = function(state)
while state do
   task.wait()
   Enemies = game:GetService("Workspace").SpawnedEnemies:GetChildren()
   for i = 1, #Enemies do
       local v = Enemies[i]
       if v:IsA("Model") and v:FindFirstChildOfClass("Humanoid") and v:FindFirstChildOfClass("Humanoid").Health > 0 then
           game.Players.LocalPlayer.Character.PrimaryPart.CFrame =
               CFrame.new(v.PrimaryPart.Position + Vector3.new(0, 0, 9), v.PrimaryPart.Position)
       end
       if game.Players.LocalPlayer:DistanceFromCharacter(v.PrimaryPart.Position) < 10 then
       end
      
   end
end
		end
})


Tab:CreateToggle({
	Name = "Auto On/Off Teleport Mob Toggle",
	CurrentValue = false,
	Flag = "Toggle1", 
	Callback = function(value)
    ddri = value
    while ddri do task.wait(1)
    automob:Set(true)
    automob:Set(false)
    end
    end,
})

local autodamage = Tab:CreateToggle({
	Name = "Bring Mobs",
	CurrentValue = false,
	Flag = "Toggle1", 
	Callback = function(value)
    getgenv().bri = value
    while getgenv().bri do
        task.wait()
        pcall(function()
            local chara = game.Players.LocalPlayer.Character.HumanoidRootPart
            for p, enemy in pairs(game:GetService("Workspace").SpawnedEnemies:GetChildren()) do
                enemy.HumanoidRootPart.CFrame = CFrame.new(chara.Position + chara.CFrame.LookVector * 7)
            end
        end)
    end
    end,
})


local Tab1 = Window:CreateTab("Egg")
Tab1:CreateSection("Auto Hatch")

Tab1:CreateToggle({
	Name = "Auto Equip Best Sword",
	CurrentValue = false,
	Flag = "Open Stars",
	Callback = function(Value)
	autobest = Value
    while autobest do wait()
local args = {[1] = "SWORD_EQUIP_BEST"}game:GetService("ReplicatedStorage").Actions.Action:FireServer(unpack(args))
    end
	end,
})

local eggs ={}

for _,v in pairs(game:GetService("Workspace").Lobby["All Eggs"]:GetChildren()) do
    table.insert(eggs,v.Name)
end

local eggtoopen
local Dropdown = Tab1:CreateDropdown({
	Name = "Egg to open ",
	Options = eggs,
	CurrentOption = "",
	Flag = "Star to open",
	Callback = function(Option)
        eggtoopen = Option
	end,
})

local openegg
Tab1:CreateToggle({
	Name = "Open Egg",
	CurrentValue = false,
	Flag = "Open Stars",
	Callback = function(Value)
		openegg = Value
        if Value then
            spawn(function()
                game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = game:GetService("Workspace").Lobby["All Eggs"][eggtoopen]["Stand"].CFrame * CFrame.new(0,0,6)
                openegg_func()
            end)
        end
	end,
})

function openegg_func()
    spawn(function()
        while wait(0.5) and openegg do
local args = {
    [1] = eggtoopen,
    [2] = 1
}

game:GetService("ReplicatedStorage").Remotes.PurchaseEgg:InvokeServer(unpack(args))

        end
    end)
end

local Tab5 = Window:CreateTab("Misc", 4483362458)

Tab5:CreateSection("Discord")

local Button = Tab5:CreateButton({
   Name = "Copy Discord Invite",
   Callback = function()
   setclipboard("discord.gg/RVnjYRWNA9")
   end,
})

Tab5:CreateSection("Movement")

Tab5:CreateToggle({
	Name = "Inf Jump",
	CurrentValue = false,
	Flag = "Toggle50", 
	Callback = function(Value)
    antiAfk = Value
            while antiAfk do task.wait(0.1)
local Player = game:GetService'Players'.LocalPlayer;
local UIS = game:GetService'UserInputService';
 
_G.JumpHeight = 50;
 
function Action(Object, Function) if Object ~= nil then Function(Object); end end
 
UIS.InputBegan:connect(function(UserInput)
    if UserInput.UserInputType == Enum.UserInputType.Keyboard and UserInput.KeyCode == Enum.KeyCode.Space then
        Action(Player.Character.Humanoid, function(self)
            if self:GetState() == Enum.HumanoidStateType.Jumping or self:GetState() == Enum.HumanoidStateType.Freefall then
                Action(self.Parent.HumanoidRootPart, function(self)
                    self.Velocity = Vector3.new(0, _G.JumpHeight, 0);
                end)
            end
        end)
    end
end)
		end
	end,
})

local walkspeed
local delrunning = false
Tab5:CreateSlider({
	Name = "Walk Speed (0 for default running)",
	Range = {0, 500},
	Increment = 10,
	Suffix = "",
	CurrentValue = 0,
	Flag = "Walk Speed",
	Callback = function(Value)
		walkspeed = Value
        if Value ~= 0 then
            delrunning = true
            game.Players.LocalPlayer.Character.Humanoid.WalkSpeed = Value
        end
	end,
})

local VirtualUser=game:service'VirtualUser'
Tab5:CreateToggle({
	Name = "Anti Afk",
	CurrentValue = true,
	Flag = "Toggle50", 
	Callback = function(Value)
    antiAfk = Value
            while antiAfk do task.wait()
game:service'Players'.LocalPlayer.Idled:connect(function()
VirtualUser:CaptureController()
VirtualUser:ClickButton2(Vector2.new())
end)
		end
	end,
})

