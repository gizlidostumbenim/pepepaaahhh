local Tab3 = Window:CreateTab("Auto open", 4483362458)


Tab3:CreateSection("Opener")


local eggs ={}

for _,v in pairs(game:GetService("Workspace").Eggs:GetChildren()) do
    table.insert(eggs,v.Name)
end

local eggtoopen
Tab3:CreateDropdown({
	Name = "Star to open",
	Options = eggs,
	CurrentOption = "",
	Flag = "Star to open",
	Callback = function(Option)
        eggtoopen = Option
	end,
})

local openegg
Tab3:CreateToggle({
	Name = "Open Stars",
	CurrentValue = false,
	Flag = "Open Stars",
	Callback = function(Value)
		openegg = Value
        if Value then
            spawn(function()
                game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = game:GetService("Workspace").Eggs[eggtoopen]["View"].CFrame
                openegg_func()
            end)
        end
	end,
})




function openegg_func()
    spawn(function()
        while wait() and openegg do
local args = {
    [1] = eggtoopen,
    [2] = "Buy8",
    [3] = true
}
game:GetService("ReplicatedStorage").GameClient.Events.RemoteFunction.BuyEgg:InvokeServer(unpack(args))
        end
    end)
end
-- ----------------------------- area teleports ----------------------------- --

local Tab5 = Window:CreateTab("Misc", 4483362458)

Tab5:CreateSection("Discord")

local Button = Tab5:CreateButton({
   Name = "Copy Discord Invite",
   Callback = function()
   setclipboard("discord.gg/RVnjYRWNA9")
   end,
})


Tab5:CreateSection("Misc")

Tab5:CreateSection("Movement")

Tab5:CreateToggle({
	Name = "Inf Jump",
	CurrentValue = false,
	Flag = "Toggle50", 
	Callback = function(Value)
    antiAfk = Value
            while antiAfk do task.wait(0.1)
local Player = game:GetService'Players'.LocalPlayer;
local UIS = game:GetService'UserInputService';
 
_G.JumpHeight = 50;
 
function Action(Object, Function) if Object ~= nil then Function(Object); end end
 
UIS.InputBegan:connect(function(UserInput)
    if UserInput.UserInputType == Enum.UserInputType.Keyboard and UserInput.KeyCode == Enum.KeyCode.Space then
        Action(Player.Character.Humanoid, function(self)
            if self:GetState() == Enum.HumanoidStateType.Jumping or self:GetState() == Enum.HumanoidStateType.Freefall then
                Action(self.Parent.HumanoidRootPart, function(self)
                    self.Velocity = Vector3.new(0, _G.JumpHeight, 0);
                end)
            end
        end)
    end
end)
		end
	end,
})

local walkspeed
local delrunning = false
Tab5:CreateSlider({
	Name = "Walk Speed (0 for default running)",
	Range = {0, 500},
	Increment = 10,
	Suffix = "",
	CurrentValue = 0,
	Flag = "Walk Speed",
	Callback = function(Value)
		walkspeed = Value
        if Value ~= 0 then
            delrunning = true
            game.Players.LocalPlayer.Character.Humanoid.WalkSpeed = Value
        end
	end,
})

local VirtualUser=game:service'VirtualUser'
Tab5:CreateToggle({
	Name = "Anti Afk",
	CurrentValue = true,
	Flag = "Toggle50", 
	Callback = function(Value)
    antiAfk = Value
            while antiAfk do task.wait()
game:service'Players'.LocalPlayer.Idled:connect(function()
VirtualUser:CaptureController()
VirtualUser:ClickButton2(Vector2.new())
end)
		end
	end,
})

local Button = Tab5:CreateButton({
   Name = "Rejoin",
   Callback = function()
   loadstring(game:HttpGet("https://pastebinp.com/raw/mM7JBG5h"))()
   end,
})

local Button = Tab5:CreateButton({
   Name = "Reset",
   Callback = function()
   loadstring(game:HttpGet("https://pastebinp.com/raw/EEY6SATj"))()
   end,
})



